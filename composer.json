{
	"name": "frozennode/administrator",
	"description": "A database interface package for Laravel",
	"homepage": "http://administrator.frozennode.com",
	"keywords": ["administrator", "admin", "database", "laravel-administrator", "laravel", "cms"],
	"license": "MIT",
	"authors": [
	{
		"name": "Jan Hartigan",
		"email": "jan@frozennode.com"
	}
	],
	"support": {
		"issues": "https://github.com/FrozenNode/Laravel-Administrator/issues"
		},
		"require": {
			"php": ">=5.4.0",
			"laravel/framework": "5.*",
			"ckeditor/ckeditor": "4.*",
			"sunhater/kcfinder": "dev-master"
			},
			"require-dev": {
				"mockery/mockery": "~0.9"
				},
				"autoload": {
					"classmap": [
					"src/controllers"
					],
					"psr-0": {
						"Frozennode\\Administrator": "src/"
					}
					},
					"scripts": {
						"post-update-cmd": [
						"if [ -d \"public/js/ckeditor/\" ] && [ -d \"vendor/ckeditor/ckeditor\" ] \nthen \nrm -R public/js/ckeditor/ \nfi ",
						"if [ -d \"vendor/ckeditor/ckeditor/\" ] \nthen \nmv vendor/ckeditor/ckeditor/ public/js/ \nfi",
						"if [ -d \"public/js/ckeditor/.git/\" ] \nthen \nrm -R public/js/ckeditor/.git/ \nfi ",
						"if [ -d \"vendor/ckeditor/\" ] \nthen \nrm -R vendor/ckeditor/ \nfi",
						"if [ -d \"public/js/kcfinder/\" ] && [ -d \"vendor/ckeditor/kcfinder\" ] \nthen \nrm -R public/js/kcfinder/ \nfi ",
						"if [ -d \"vendor/sunhater/kcfinder/\" ] \nthen \nmv vendor/sunhater/kcfinder/ public/js/ \nfi",
						"if [ -d \"public/js/kcfinder/.git/\" ] \nthen \nrm -R public/js/kcfinder/.git/ \nfi ",
						"if [ -d \"vendor/sunhater/\" ] \nthen \nrm -R vendor/sunhater/ \nfi"
						]
						},
						"minimum-stability": "dev"
					}
